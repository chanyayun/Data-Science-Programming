library(readr)
garbage_file <- read.csv("garbage.csv")
popularity_file <- read_csv("popularity.csv")
str(garbage_file)
str(popularity_file)
#garbage_file$GarbageGenerated <- as.numeric(gsub(",", "", garbage_file$GarbageGenerated))
g <- garbage_file %>%
filter(year == "106", month == "11") %>%
select(county, GarbageGenerated) %>%
rename(
地區 = county,
`垃圾生產量(t)` = GarbageGenerated
)
str(g)
View(g)
g$`垃圾生產量(t)` <- as.numeric(gsub(",", "", g$`垃圾生產量(t)`))
View(g)
p <- data.frame(popularity_file) %>%
filter(!grepl("區", 地區), !grepl("省", 地區)) %>%
select(地區, 年底人口數)
merged_data <- merge(p, g, by = "地區")
View(merged_data)
merged_data <- merged_data[order(merged_data$年底人口數), ]
merged_data <- merged_data[order(col(2)), ]
